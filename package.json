{
  "name": "aws-console",
  "version": "1.0.2",
  "description": "AWS Console",
  "main": "main.js",
  "build": {
    "appId": "jnawk.aws-console",
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "files": [
      "build/*",
      "*js",
      "*html"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "webpack",
    "start": "electron .",
    "prepare": "webpack",
    "lint": "eslint *js && eslint app/*",
    "pack": "electron-builder --dir",
    "dist": "webpack && electron-builder --linux --mac && bash -c \"docker run --rm -ti --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS_TAG|TRAVIS|TRAVIS_REPO_|TRAVIS_BUILD_|TRAVIS_BRANCH|TRAVIS_PULL_REQUEST_|APPVEYOR_|CSC_|GH_|GITHUB_|BT_|AWS_|STRIP|BUILD_')  --env ELECTRON_CACHE=/root/.cache/electron --env ELECTRON_BUILDER_CACHE=/root/.cache/electron-builder -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.cache/electron:/root/.cache/electron  -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine bash -c \\\"npm i && npm run docker-dist-win\\\"\"",
    "docker-dist-win": "electron-builder --windows"
  },
  "author": "Philip Dowie <philip@jnawk.nz>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "ajv": "^6.12.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.5.2",
    "css-loader": "^4.3.0",
    "electron": "^9.3.2",
    "electron-builder": "^22.8.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^1.0.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.771.0",
    "electron-tabs": "^0.15.0",
    "ini": "^1.3.5",
    "node-fetch": "^2.6.1",
    "proxy-agent": "^4.0.0",
    "query-string": "^6.13.5",
    "split-ca": "^1.0.1"
  }
}
