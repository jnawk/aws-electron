{
  "name": "aws-console",
  "version": "1.0.0",
  "description": "AWS Console",
  "main": "main.js",
  "build": {
    "appId": "jnawk.aws-console",
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "files": [
      "build/*",
      "*js",
      "*html"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "webpack",
    "start": "electron .",
    "prepare": "webpack",
    "lint": "eslint *js && eslint app/*",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl"
  },
  "author": "Philip Dowie <philip@jnawk.nz>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "ajv": "^6.12.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.5.0",
    "css-loader": "^2.1.1",
    "electron": "^9.3.2",
    "electron-builder": "^22.8.1",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.2",
    "file-loader": "^2.0.0",
    "jquery": "^3.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^6.5.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "aws-sdk": "^2.761.0",
    "electron-tabs": "^0.15.0",
    "ini": "^1.3.5",
    "node-fetch": "^2.6.1",
    "proxy-agent": "^3.1.1",
    "query-string": "^6.13.1",
    "split-ca": "^1.0.1"
  }
}
