{
  "name": "aws-console",
  "version": "1.4.0",
  "description": "AWS Console",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnawk/aws-electron.git"
  },
  "build": {
    "appId": "jnawk.aws-console",
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "portable",
        "nsis",
        "7z",
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "files": [
      "build/*",
      "*js",
      "*html"
    ]
  },
  "scripts": {
    "test": "mocha",
    "prestart": "webpack",
    "start": "electron .",
    "prepare": "webpack",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext js,jsx .",
    "pack": "electron-builder --dir",
    "dist": "webpack && electron-builder --linux --mac && npm run dist:win",
    "dist:win": "bash -c \"docker run --rm -ti --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS_TAG|TRAVIS|TRAVIS_REPO_|TRAVIS_BUILD_|TRAVIS_BRANCH|TRAVIS_PULL_REQUEST_|APPVEYOR_|CSC_|GH_|GITHUB_|BT_|AWS_|STRIP|BUILD_')  --env ELECTRON_CACHE=/root/.cache/electron --env ELECTRON_BUILDER_CACHE=/root/.cache/electron-builder -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.cache/electron:/root/.cache/electron  -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine bash -c \\\"npm i -g npm && npm i && npm run dist:win:docker\\\"\"",
    "dist:win:docker": "electron-builder --windows"
  },
  "author": "Philip Dowie <philip@jnawk.nz>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "ajv": "^6.12.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4",
    "chai": "^4",
    "css-loader": "^5",
    "electron": "^10",
    "electron-builder": "^22.9.1",
    "eslint": "^7",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-jsdoc": "^32",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-react": "^7",
    "file-loader": "^6.2.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^1",
    "mocha": "^8",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5",
    "webpack-cli": "^4"
  },
  "dependencies": {
    "aws-sdk": "^2",
    "electron-tabs": "^0.15.0",
    "ini": "^2.0.0",
    "node-fetch": "^2.6.1",
    "proxy-agent": "^4",
    "query-string": "^6",
    "split-ca": "^1.0.1"
  }
}
