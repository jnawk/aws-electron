{
  "name": "aws-console",
  "author": "Philip Dowie <philip@jnawk.nz>",
  "description": "AWS Console",
  "license": "ISC",
  "version": "2.1.2",
  "main": "./dist/main.bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnawk/aws-electron.git"
  },
  "bugs": {
    "url": "https://github.com/jnawk/aws-electron/issues"
  },
  "keywords": [
    "AWS"
  ],
  "homepage": "https://github.com/jnawk/aws-electron/blob/master/README.md",
  "build": {
    "appId": "jnawk.aws-console",
    "productName": "AWS Console",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "zip"
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "nsis",
        "7z",
        "zip"
      ],
      "asar": true
    },
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage",
        "zip",
        "tar.gz"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "scripts": {
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "devOne": "rimraf dist && cross-env NODE_ENV=development webpack --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "dist": "npm i; npm run prod; electron-builder build --linux --mac --x64; npm run dist:win; npm i",
    "dist:mac": "electron-builder build --mac --x64",
    "dist:linux": "electron-builder build --linux --x64",
    "dist:win": "bash -c \"docker run --rm -ti --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS_TAG|TRAVIS|TRAVIS_REPO_|TRAVIS_BUILD_|TRAVIS_BRANCH|TRAVIS_PULL_REQUEST_|APPVEYOR_|CSC_|GH_|GITHUB_|BT_|AWS_|STRIP|BUILD_') --env ELECTRON_CACHE=/.cache/electron --env ELECTRON_BUILDER_CACHE=/.cache/electron-builder -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.cache/electron:/.cache/electron -v ~/.cache/electron-builder:/.cache/electron-builder electronuserland/builder:wine bash -c \\\"chown -R 1000 /root; chown root /root/.wine -R; npm i -g npm && npm i && npm run dist:win:docker\\\"\"",
    "dist:win:docker": "electron-builder --windows"
  },
  "dependencies": {
    "@aws-sdk/client-iam": "^3.51.0",
    "@aws-sdk/client-sts": "^3.51.0",
    "@aws-sdk/credential-provider-ini": "^3.51.0",
    "@aws-sdk/credential-providers": "^3.51.0",
    "@aws-sdk/node-http-handler": "^3.50.0",
    "@aws-sdk/types": "^3.50.0",
    "bootstrap": "^5",
    "debounce": "^1.2.1",
    "electron-context-menu": "^3",
    "electron-settings": "^4.0.2",
    "electron-tabs": "^0.15.0",
    "ini": "^2.0.0",
    "proxy-agent": "^4",
    "query-string": "^7",
    "react": "^17",
    "react-dom": "^17",
    "react-router": "^6",
    "react-router-dom": "^6",
    "react-transition-group": "^4.4.2",
    "reactstrap": "^9",
    "split-ca": "^1.0.1",
    "urllib": "^2.38.0"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.1",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^8.2.0",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-router": "^5",
    "@types/react-router-dom": "^5",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "copy-webpack-plugin": "^9",
    "cross-env": "^7.0.3",
    "css-loader": "^6",
    "electron": "^11.1.1",
    "electron-builder": "^22",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^19",
    "eslint-import-resolver-webpack": "^0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5",
    "lodash": "^4",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3",
    "spectron": "^19.0.0",
    "style-loader": "^3",
    "ts-loader": "^9",
    "ts-node": "^10",
    "tsconfig-paths": "^3",
    "typescript": "^4",
    "webpack": "^5",
    "webpack-cli": "^4"
  }
}
